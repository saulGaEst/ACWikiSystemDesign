<script>
            function copyCurrentUrlToClipboard(event) {
        event.preventDefault();

        // Obtener la URL actual
        const currentUrl = window.location.href;

        // Método alternativo de copia que funciona en más navegadores
        try {
            // Crear un elemento de texto temporal
            const tempInput = document.createElement('input');
            tempInput.value = currentUrl;
            document.body.appendChild(tempInput);
            tempInput.select();
            document.execCommand('copy');
            document.body.removeChild(tempInput);

            // Mostrar el toast
            const toast = document.getElementById('shareToast');
            toast.style.display = 'block';

            // Animar el botón
            const button = event.currentTarget;
            button.classList.remove('btn-outline-secondary');
            button.classList.add('btn-outline-dark');

            // Restaurar el botón y ocultar el toast después de 1.5 segundos
            setTimeout(() => {
                button.classList.remove('btn-outline-dark');
                button.classList.add('btn-outline-secondary');
                toast.style.display = 'none';
            }, 1500);
        } catch (err) {
            console.error('Error al copiar: ', err);
        }
    }

    // Asegurarse de que el toast se añada al body
    document.addEventListener('DOMContentLoaded', function() {
        if (!document.getElementById('shareToast')) {
            const toastDiv = document.createElement('div');
            toastDiv.innerHTML = `
                <div id="shareToast" class="position-fixed bottom-0 end-0 p-3" style="z-index: 1050; right: 20px; bottom: 20px; display: none;">
                    <div class="toast g-bg-gray-dark-v2 text-white" role="alert" aria-live="assertive" aria-atomic="true">
                        <div class="toast-body d-flex align-items-center">
                            <i class="fa fa-check-circle mr-2"></i>
                            <span>¡Enlace copiado al portapapeles!</span>
                        </div>
                    </div>
                </div>
            `;
            document.body.appendChild(toastDiv.firstElementChild);
        }
    });
</script>
