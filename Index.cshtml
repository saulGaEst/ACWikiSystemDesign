<script>
document.addEventListener('DOMContentLoaded', function() {
        // Elementos del DOM
    const vehiclePrice = document.getElementById('vehiclePrice');
    const downPaymentSlider = document.getElementById('downPaymentSlider');
    const downPaymentAmount = document.getElementById('downPaymentAmount');
    const termSlider = document.getElementById('termSlider');
    const termAmount = document.getElementById('termAmount');
    const interestRate = document.getElementById('interestRate');

    // Elementos de resultado
    const monthlyPayment = document.getElementById('monthlyPayment');
    const financedAmount = document.getElementById('financedAmount');
    const totalPayment = document.getElementById('totalPayment');
    const totalInterest = document.getElementById('totalInterest');

    // Función para formatear números como moneda
    function formatCurrency(amount) {
        return new Intl.NumberFormat('es-MX', {
            style: 'currency',
            currency: 'MXN',
            minimumFractionDigits: 0,
            maximumFractionDigits: 0
        }).format(amount);
    }

    // Función para formatear input con comas
    function formatNumberInput(value) {
        // Remover todo excepto números
        const numericValue = value.replace(/[^\d]/g, '');
        // Agregar comas como separadores de miles
        return numericValue.replace(/\B(?=(\d{3})+(?!\d))/g, ',');
    }

    // Función para obtener valor numérico del input
    function getNumericValue(value) {
        return parseFloat(value.replace(/[,$]/g, '')) || 0;
    }

    // Función para calcular el pago mensual
    function calculateLoan() {
        const price = getNumericValue(vehiclePrice.value);
        const downPayment = parseFloat(downPaymentSlider.value);

        // Validar que el precio sea válido
        if (price <= 0) {
            monthlyPayment.textContent = '$0';
            financedAmount.textContent = '$0';
            totalPayment.textContent = '$0';
            totalInterest.textContent = '$0';
            return;
        }

        // Validar que el enganche no sea mayor al precio
        const maxDownPayment = Math.min(price * 0.9, downPayment); // Máximo 90% del precio
        const actualDownPayment = Math.min(downPayment, maxDownPayment);

        const principal = price - actualDownPayment;
        const monthlyRate = parseFloat(interestRate.value) / 100 / 12;
        const numberOfPayments = parseInt(termSlider.value);

        // Validar que haya monto a financiar
        if (principal <= 0) {
            monthlyPayment.textContent = '$0';
            financedAmount.textContent = '$0';
            totalPayment.textContent = '$0';
            totalInterest.textContent = '$0';
            return;
        }

        // Fórmula de pago mensual
        const monthlyPaymentAmount = principal * (monthlyRate * Math.pow(1 + monthlyRate, numberOfPayments)) /
                                   (Math.pow(1 + monthlyRate, numberOfPayments) - 1);

        const totalPaymentAmount = monthlyPaymentAmount * numberOfPayments;
        const totalInterestAmount = totalPaymentAmount - principal;

        // Actualizar la interfaz
        monthlyPayment.textContent = formatCurrency(monthlyPaymentAmount);
        financedAmount.textContent = formatCurrency(principal);
        totalPayment.textContent = formatCurrency(totalPaymentAmount);
        totalInterest.textContent = formatCurrency(totalInterestAmount);
    }

    // Función para actualizar límites del slider de enganche
    function updateDownPaymentLimits() {
        const price = getNumericValue(vehiclePrice.value);

        if (price > 0) {
            const minDownPayment = Math.max(10000, price * 0.1); // Mínimo 10% o $10,000
            const maxDownPayment = Math.min(price * 0.9, 500000); // Máximo 90% o $500,000

            downPaymentSlider.min = minDownPayment;
            downPaymentSlider.max = maxDownPayment;

            // Ajustar valor actual si está fuera de rango
            const currentDownPayment = parseFloat(downPaymentSlider.value);
            if (currentDownPayment < minDownPayment) {
                downPaymentSlider.value = minDownPayment;
            } else if (currentDownPayment > maxDownPayment) {
                downPaymentSlider.value = maxDownPayment;
            }

            downPaymentAmount.textContent = formatCurrency(downPaymentSlider.value);
        }
    }

    // Event listener para el precio del vehículo
    vehiclePrice.addEventListener('input', function() {
        // Formatear el input mientras el usuario escribe
        const cursorPosition = this.selectionStart;
        const oldLength = this.value.length;

        this.value = formatNumberInput(this.value);

        // Mantener la posición del cursor
        const newLength = this.value.length;
        const newCursorPosition = cursorPosition + (newLength - oldLength);
        this.setSelectionRange(newCursorPosition, newCursorPosition);

        updateDownPaymentLimits();
        calculateLoan();
    });

    // Event listener para cuando el usuario termina de editar el precio
    vehiclePrice.addEventListener('blur', function() {
        const price = getNumericValue(this.value);
        if (price < 50000) {
            alert('El precio mínimo del vehículo debe ser de $50,000');
            this.value = '50,000';
            updateDownPaymentLimits();
            calculateLoan();
        }
    });

    // Event listeners para los sliders
    downPaymentSlider.addEventListener('input', function() {
        downPaymentAmount.textContent = formatCurrency(this.value);
        calculateLoan();
    });

    termSlider.addEventListener('input', function() {
        termAmount.textContent = this.value;
        calculateLoan();
    });

    interestRate.addEventListener('change', function() {
        calculateLoan();
    });

    // Inicializar
    updateDownPaymentLimits();
    downPaymentAmount.textContent = formatCurrency(downPaymentSlider.value);
    termAmount.textContent = termSlider.value;
    calculateLoan();
    });
</script>
</html>
